<fieldset class="usa-fieldset">
  <%= form.input :lg_account_id,
      label: "<b>Account ID (e.g. LG-E-DOT)</b>".html_safe, label_html: { class: 'usa-label usa-input-required' }, input_html: { class: 'usa-input' }, required: true %>
  <%= form.input :name,
      label: "<b>Name</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :lg_agency_id,
      label: "<b>Agency ID</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :iaa_active, as: :boolean, checked_value: true, unchecked_value: false,
      label: "<b>IAA active</b>".html_safe, wrapper_html: { class: 'usa-checkbox margin-top-3' }, label_html: { class: '' }, input_html: { class: 'margin-right-1' } %>
  <%= form.input :iaa_7600a,
      label: "<b>7600A (GT&C#)</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :iaa_7600a_start,
      label: "<b>7600A start date</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :iaa_7600a_end,
      label: "<b>7600A end date</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :iaa_7600a_amount,
      label: "<b>7600A amount</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :iaa_7600b,
      label: "<b>7600B (order #)</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :iaa_7600b_start,
      label: "<b>7600B start date</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :iaa_7600b_end,
      label: "<b>7600B end date</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :iaa_7600b_amount,
      label: "<b>7600B amount</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :pricing,
      label: "<b>Pricing model version</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
  <%= form.input :became_partner,
      label: "<b>Became partner</b>".html_safe, label_html: { class: 'usa-label' }, input_html: { class: 'usa-input' } %>
</fieldset>
